    "devDependencies": {
        "@hkamran/eslint-config": "^1.0.1",
        "@hkamran/prettier-config": "^1.1.1",
        "@types/node": "^20.4.8",
        "@typescript-eslint/eslint-plugin": "^6.2.1",
        "@typescript-eslint/parser": "^6.2.1",
        "eslint": "^8.46.0",
